<div class="container-new-service">
  <h1>Add your service!</h1>

  <%= simple_form_for @service do |f| %>
    <%= f.input :name %>
    <%= f.input :category, collection: ['Dinner', 'Sport', 'Concert', 'Time'] %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.submit %>
  <% end %>
</div>
